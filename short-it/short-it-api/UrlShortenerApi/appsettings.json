{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "UrlShortenerDbOptions": {
    "Host": "localhost",
    "Database": "url",
    "Username": "sa",
    "Password": "admin@1234"
  },
  "AllowedHosts": "*"
}
